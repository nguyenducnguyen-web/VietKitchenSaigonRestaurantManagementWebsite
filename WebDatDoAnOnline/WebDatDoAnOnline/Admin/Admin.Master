    <style>
        :root {
            --primary-color: #ff6f61;
            --secondary-color: #ffca28;
            --accent-color: #ff0000;
            --bg-dark: #1a202c;
            --bg-light: #ffffff;
            --text-dark: #2d3748;
            --text-light: #ffffff;
        }

        [data-theme="dark"] {
            --bg-main: var(--bg-dark);
            --text-main: var(--text-light);
            --card-bg: #2d3748;
        }

        [data-theme="light"] {
            --bg-main: var(--bg-light);
            --text-main: var(--text-dark);
            --card-bg: #f7fafc;
        }

        body {
            font-family: 'Inter', sans-serif;
            background: var(--bg-main);
            color: var(--text-main);
            margin: 0;
            overflow-x: hidden;
            transition: background 0.3s ease, color 0.3s ease;
        }

        #particles-js {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            z-index: -1;
            opacity: 0.5;
        }

        /* Header */
        .pcoded-header {
            background: linear-gradient(90deg, var(--primary-color), var(--secondary-color));
            box-shadow: 0 4px 20px rgba(0, 0, 0, 0.2);
            padding: 0.75rem 1.5rem;
            position: fixed;
            width: 100%;
            z-index: 1000;
            display: flex;
            align-items: center;
            justify-content: space-between;
        }

        .navbar-logo img {
            width: 100px;
            transition: transform 0.3s ease;
        }

        .navbar-logo img:hover {
            transform: scale(1.05);
        }

        .user-profile img {
            width: 32px;
            height: 32px;
            border-radius: 50%;
            border: 2px solid var(--secondary-color);
            transition: transform 0.3s ease;
        }

        .user-profile img:hover {
            transform: scale(1.1);
        }

        .user-profile span {
            font-size: 0.9rem;
            font-weight: 500;
        }

        .profile-notification {
            background: var(--card-bg);
            border-radius: 12px;
            box-shadow: 0 8px 24px rgba(0, 0, 0, 0.2);
            padding: 0.5rem;
            min-width: 200px;
        }

        .profile-notification li a {
            color: var(--text-main);
            padding: 0.75rem 1rem;
            display: block;
            transition: background 0.2s ease;
        }

        .profile-notification li a:hover {
            background: var(--primary-color);
            color: var(--text-light);
            border-radius: 8px;
        }

        /* Sidebar */
        .pcoded-navbar {
            background: var(--card-bg);
            width: 280px;
            position: fixed;
            top: 64px;
            bottom: 0;
            transition: transform 0.3s ease;
            z-index: 999;
            box-shadow: 2px 0 10px rgba(0, 0, 0, 0.1);
        }

        .pcoded-navigatio-lavel {
            color: var(--primary-color);
            font-size: 0.9rem;
            font-weight: 600;
            text-transform: uppercase;
            padding: 1.5rem 1rem;
            position: relative;
            margin-bottom: 0.5rem;
        }

        .pcoded-navigatio-lavel::after {
            content: '';
            position: absolute;
            bottom: 0.5rem;
            left: 1rem;
            width: 30px;
            height: 1px;
            background: var(--secondary-color);
            opacity: 0.7;
            transition: width 0.3s ease, opacity 0.3s ease;
        }

        .pcoded-navigatio-lavel:hover::after {
            width: 50px;
            opacity: 1;
        }

        .pcoded-item li a {
            color: var(--text-main);
            font-size: 0.95rem;
            padding: 0.75rem 1.5rem;
            display: flex;
            align-items: center;
            border-radius: 8px;
            margin: 0.25rem 0.75rem;
            transition: all 0.2s ease;
        }

        .pcoded-item li a:hover,
        .pcoded-item li.active a {
            background: var(--primary-color);
            color: var(--text-light);
            transform: translateX(5px);
        }

        .pcoded-micon i {
            color: var(--secondary-color);
            margin-right: 0.75rem;
            font-size: 1.1rem;
        }

        /* Main Content */
        .pcoded-main-container {
            margin-left: 280px;
            padding: 84px 1.5rem 1.5rem;
            min-height: 100vh;
        }

        .pcoded-content {
            background: var(--card-bg);
            border-radius: 16px;
            box-shadow: 0 8px 24px rgba(0, 0, 0, 0.1);
            padding: 2rem;
            animation: fadeIn 0.5s ease;
        }

        /* Breadcrumb */
        .danger-breadcrumb {
            background: linear-gradient(90deg, var(--primary-color), var(--accent-color));
            border-radius: 12px;
            padding: 1rem;
            margin-bottom: 1.5rem;
        }

        .breadcrumb-title li a {
            color: var(--text-light);
            font-size: 0.9rem;
            font-weight: 500;
            transition: color 0.2s ease;
        }

        .breadcrumb-title li a:hover {
            color: var(--secondary-color);
        }

        /* Loader */
        .theme-loader {
            background: rgba(0, 0, 0, 0.8);
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            z-index: 9999;
            display: flex;
            align-items: center;
            justify-content: center;
        }

        .ball-scale .contain .ring .frame {
            background: var(--secondary-color);
            border-radius: 50%;
            animation: pulse 1.5s infinite;
        }

        /* SweetAlert2 */
        .custom-swal-popup {
            background: linear-gradient(135deg, var(--primary-color), var(--accent-color)) !important;
            border-radius: 16px !important;
            box-shadow: 0 10px 30px rgba(0, 0, 0, 0.3) !important;
        }

        .custom-swal-title,
        .custom-swal-text {
            color: var(--text-light) !important;
        }

        .custom-swal-button {
            background: var(--accent-color) !important;
            color: var(--text-light) !important;
            border-radius: 12px !important;
            padding: 0.75rem 2rem !important;
        }

        .custom-swal-button:hover {
            background: #ff3333 !important;
        }

        /* Animations */
        @keyframes fadeIn {
            from { opacity: 0; transform: translateY(20px); }
            to { opacity: 1; transform: translateY(0); }
        }

        @keyframes pulse {
            0% { transform: scale(1); opacity: 1; }
            50% { transform: scale(1.3); opacity: 0.6; }
            100% { transform: scale(1); opacity: 1; }
        }

        /* Mobile Styles */
        @media (max-width: 1024px) {
            .pcoded-navbar {
                transform: translateX(-100%);
            }

            .pcoded-navbar.active {
                transform: translateX(0);
            }

            .pcoded-main-container {
                margin-left: 0;
            }
        }
    </style>

    <script type="text/javascript" src="../assets/js/jquery/jquery.min.js"></script>
    <script>
        $(function () {
            // Theme Toggle
            const themeToggle = $('<button class="text-white p-2 rounded-full hover:bg-opacity-20 hover:bg-white"><i class="fas fa-moon"></i></button>');
            $('.nav-right').prepend(themeToggle);
            themeToggle.click(() => {
                const currentTheme = $('html').attr('data-theme') === 'dark' ? 'light' : 'dark';
                $('html').attr('data-theme', currentTheme);
                themeToggle.find('i').toggleClass('fa-moon fa-sun');
            });

            // Active Menu Highlight
            var url = window.location.href;
            $("#navbarSupportedContent a").each(function () {
                if (url == this.href) {
                    $(this).closest("li").addClass("active");
                }
            });

            // Particles.js
            particlesJS("particles-js", {
                particles: {
                    number: { value: 80, density: { enable: true, value_area: 800 } },
                    color: { value: ["#ff6f61", "#ffca28", "#ff0000"] },
                    shape: { type: "circle" },
                    opacity: { value: 0.5, random: true },
                    size: { value: 3, random: true },
                    move: { enable: true, speed: 1, direction: "none", random: true, out_mode: "out" }
                },
                interactivity: {
                    events: { onhover: { enable: true, mode: "repulse" }, onclick: { enable: true, mode: "push" } },
                    modes: { repulse: { distance: 100 }, push: { particles_nb: 5 } }
                },
                retina_detect: true
            });

            // Mobile Menu Toggle
            $('.mobile-menu').click(function () {
                $('.pcoded-navbar').toggleClass('active');
            });

            // Fullscreen Toggle
            window.toggleFullScreen = function () {
                if (!document.fullscreenElement) {
                    document.documentElement.requestFullscreen();
                } else {
                    if (document.exitFullscreen) {
                        document.exitFullscreen();
                    }
                }
            };
        });

        function showSuccessAlert(message) {
            Swal.fire({
                title: 'Thành công!',
                html: message,
                icon: 'success',
                customClass: {
                    popup: 'custom-swal-popup',
                    title: 'custom-swal-title',
                    htmlContainer: 'custom-swal-text',
                    confirmButton: 'custom-swal-button'
                },
                backdrop: `rgba(0, 0, 0, 0.7)`,
                didOpen: () => {
                    document.getElementById('successSound').play();
                    confetti({
                        particleCount: 150,
                        spread: 100,
                        colors: ['#ff6f61', '#ff0000', '#ffca28'],
                        origin: { y: 0.6 }
                    });
                }
            });
        }

        function showErrorAlert(message) {
            Swal.fire({
                title: 'Lỗi!',
                html: message,
                icon: 'error',
                customClass: {
                    popup: 'custom-swal-popup',
                    title: 'custom-swal-title',
                    htmlContainer: 'custom-swal-text',
                    confirmButton: 'custom-swal-button'
                },
                backdrop: `rgba(0, 0, 0, 0.7)`,
                didOpen: () => {
                    document.getElementById('errorSound').play();
                }
            });
        }
    </script>

