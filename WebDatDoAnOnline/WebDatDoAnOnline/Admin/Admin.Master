                nav.addClass('active');
            } else {
                nav.removeClass('active');
            }
        });

        // Toggle profile dropdown
        $('.user-profile').click(function (e) {
            e.stopPropagation();
            $('.profile-notification').toggleClass('hidden');
        });

        $(document).click(function (e) {
            if (!$(e.target).closest('.user-profile').length) {
                $('.profile-notification').addClass('hidden');
            }
        });
    </script>
